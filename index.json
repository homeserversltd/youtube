{
  "name": "youtube",
  "version": "1.0.0",
  "config": {
    "repository": {
      "url": "git@github.com:homeserversltd/youtube.git",
      "branch": "main"
    },
    "git_managed": true
  },
  "files": {
    "config": "homeserver.patch.json",
    "readme": "README.md",
    "license": "LICENSE",
    "backend": {
      "index": "backend/index.json",
      "requirements": "backend/requirements.txt",
      "init": "backend/__init__.py",
      "routes": "backend/routes.py",
      "youtube_manager": "backend/youtube_manager.py",
      "subscription_manager": "backend/subscription_manager.py"
    },
    "frontend": {
