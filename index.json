      "entry": "frontend/index.tsx",
      "types": "frontend/types.ts",
      "styles": "frontend/PortalCard.css",
      "DownloadForm": "frontend/components/DownloadForm.tsx",
      "SubscriptionList": "frontend/components/SubscriptionList.tsx",
      "ScheduleConfig": "frontend/components/ScheduleConfig.tsx",
      "DownloadSettings": "frontend/components/DownloadSettings.tsx",
      "LogsView": "frontend/components/LogsView.tsx",
      "useYoutubeControls": "frontend/hooks/useYoutubeControls.ts"
    },
    "permissions": {
      "sudoers": "permissions/flask-youtube"
